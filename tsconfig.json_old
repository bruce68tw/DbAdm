{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "rootDir": "./_tsBase",
    "outDir": "./wwwroot/js_new",
    "strict": true,
    "removeComments": false,
    "sourceMap": true,
    "mapRoot": "./wwwroot/map",
    "esModuleInterop": true,
    "inlineSources": true,
    "types": [ "jquery" ]
  },
  "include": [
    "_tsBase/**/*.ts",
    "_tsView/**/*.ts"
  ]
}
